<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Copyright>Copyright mlnrdns</Copyright>
    <PackageProjectUrl>https://github.com/mlnrdns/discogsharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mlnrdns/discogsharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>    
    <Authors>mlnrdns</Authors>
    <Description>discogsharp is a .NET client library to access Discogs API</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1"/>
  </ItemGroup>
</Project>